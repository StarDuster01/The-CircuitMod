        "west": "true"
      },
      "apply": {
        "model": "circuitmod:block/pipes/gaspipepart",
        "y": 270,
        "uvlock": false
      }
    },
	{
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "circuitmod:block/pipes/gaspipepart",
        "x": 270,
        "uvlock": false
